{
    "name": "Ps3NetServer",
    "slug": "ps3netserver",
    "version": "0.1.0",
    "description": "This is a Docker container for ps3netsrv (or ps3netsvr).",
    "url": "https://github.com/d-two/hassio-addons/tree/main/ps3netsrv",
    "startup": "application",
    "init": false,
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
	"armv7",
	"i386"
    ],
    "boot": "auto",
    "map": [
		"media:rw"
    ],
	"ports": {
	  "38008/tcp": 38008
    },
    "ports_description": {
	  "38008/tcp": "Network port"
    }
}
